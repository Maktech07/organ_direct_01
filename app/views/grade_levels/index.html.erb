<%= render 'statics/common_links' %><br>

<h1>Listing grade_levels</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grade_levels.each do |grade_level| %>
      <tr>
        <td><%= grade_level.number %></td>
        <td><%= grade_level.name %></td>
        <td><%= link_to 'Show', grade_level %></td>
        <td><%= link_to 'Edit', edit_grade_level_path(grade_level) %></td>
        <td><%= link_to 'Destroy', grade_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grade level', new_grade_level_path %>
